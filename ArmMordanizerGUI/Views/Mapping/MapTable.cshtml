@model Mapper

@{
    var i = 0;
}
@for ( i=0;i< Model.mapTables.Count;i++)
{
    @Html.DropDownListFor(m => m.mapTables[i].sourceColumn, new SelectList(Model.sourceSelectList, "Text", "Value"), "Default label", new { @class = "css-class" })
    @Html.DropDownListFor(m => m.mapTables[i].targetColumn, new SelectList(Model.desTinationSelectList, "Text", "Value"), "Default label", new { @class = "css-class" })

}